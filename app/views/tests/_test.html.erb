<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td>
    <% if test.questions.any? %>
      <%= button_to t('.start'), start_test_path(test) %>
    <% else %>
      <span class="text-muted"><%= t('.no_questions') %></span>
    <% end %>
  </td>
</tr>

