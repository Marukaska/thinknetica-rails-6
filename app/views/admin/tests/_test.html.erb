<tr>
  <td><%= test.title %></td>
  <td align="center"><%= test_level(test) %></td>
  <td><%= link_to t('.show'), admin_test_path(test) %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('.delete'),
                admin_test_path(test),
                data: {"turbo-method": :delete, turbo_confirm: t('.confirm')} %>
  </td>
  <td align="center"><%= pluralize(test.questions.count, 'question') %></td>
</tr>
